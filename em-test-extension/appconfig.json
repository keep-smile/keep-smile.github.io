{
    "name": "EM Test Extension",
    "provider": "BZ",
    "description": "Sample extension inside Shell with Shell navigation",   
    "version": "1.3.0",
    "icon": "https://keep-smile.github.io/em-test-extension/yes-sign.png",
    "useShellSDK": false,
    "outletPositions": ["SHELL_HOME_SCREEN", "PP_DASHBOARD_1", "PP_TECHNICIAN_DETAILS_1", "PP_ASSIGNMENT_DETAILS_2", "SERVICE_CALL_CREATE_PAGE_EQUIPMENT_TAB_TOP"],
    "insideShell": true,
    "parametersInformation": "Parameters information text. Please provide folloving parameters required for the extension setup.",
    "extensionPolicies": [],
    "parameters": [
        {
          "name": "systemUrl",
          "required": true,
          "type": "TEXT",
          "regexValidation": null,
          "labelTranslations": [
            {
              "lang": "en",
              "value": "System URL"
            }
          ],
          "toolTip": "This is the URL that is used to connect the extension to your account our System"
        }
    ],
    "shellNavigation": {
        "icon": "activities",
        "sideNavigation": [
          {
            "pathSegment": "home",
            "labelTranslations": [
              {
                "lang": "en",
                "value": "Home"
              }
            ],
            "viewUrl": "/#/",
            "icon": "home",
            "isDefault": true,
            "children": []
          }
       ]
    }
}
