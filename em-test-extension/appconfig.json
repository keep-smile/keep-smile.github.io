{
    "name": "EM Test Extension",
    "provider": "BZ",
    "description": "Sample extension inside Shell with Shell navigation",   
    "version": "1.3.0",
    "icon": "https://keep-smile.github.io/em-test-extension/yes-sign.png",
    "useShellSDK": false,
    "outletPositions": ["SHELL_HOME_SCREEN"],
    "insideShell": true,
    "parametersInformation": "Parameters information text. Please provide folloving parameters required for the extension setup.",
    "extensionPolicies": [],
    "parameters": [
        {
          "name": "systemUrl",
          "required": true,
          "type": "TEXT",
          "regexValidation": null,
          "labelTranslations": [
            {
              "lang": "en",
              "value": "System URL"
            }
          ],
          "toolTip": "This is the URL that is used to connect the extension to your account our System",
          "toolTipTranslations": [
            {
              "lang": "en",
              "value": "System URL"
            },
            {
              "lang": "de",
              "value": "System URL DE"
            }
          ] 
        },
        {
          "name": "checkbox",
          "type": "CHECKBOX",
          "labelTranslations": [
            {
              "lang": "en",
              "value": "System URL"
            }
          ],
          "toolTip": "This is the URL that is used to connect the extension to your account our System"
        }
        ,
        {
          "name": "checkbox3",
          "type": "CHECKBOX",
          "labelTranslations": [
            {
              "lang": "en",
              "value": "3"
            }
          ],
          "toolTip": "This is the URL that is used to connect the extension to your account our System"
        }
,
        {
          "name": "checkbox2",
          "type": "CHECKBOX2",
          "labelTranslations": [
            {
              "lang": "en",
              "value": "System URL"
            }
          ],
          "toolTip": "This is the URL that is used to connect the extension to your account our System"
        }

        

    ],
    "shellNavigation": {
        "icon": "activities",
        "sideNavigation": [
          {
            "pathSegment": "home",
            "labelTranslations": [
              {
                "lang": "en",
                "value": "Home"
              }
            ],
            "viewUrl": "/#/",
            "icon": "home",
            "isDefault": true,
            "children": []
          }
       ]
    }
}
