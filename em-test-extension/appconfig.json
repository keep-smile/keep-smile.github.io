{
    "name": "EM Test Extension",
    "provider": "BZ",
    "description": "Sample extension inside Shell with Shell navigation",   
    "version": "1.3.0",
    "icon": "https://keep-smile.github.io/em-test-extension/yes-sign.png",
    "useShellSDK": false,
    "outletPositions": ["SHELL_HOME_SCREEN", "PP_DASHBOARD_1", "PP_TECHNICIAN_DETAILS_1", "PP_ASSIGNMENT_DETAILS_2", "SERVICE_CALL_CREATE_PAGE_EQUIPMENT_TAB_TOP"],
    "insideShell": true,
    "parametersInformation": "Parameters information text. Please provide folloving parameters required for the extension setup.",
    "parameters": [{
      "name": "alowContact",
      "type": "BOOLEAN",
      "labelTranslations": [
        {
          "lang": "en",
          "value": "Allow contact"
        },
        {
          "lang": "de",
          "value": "Kontakt zulassen"
        }
      ]
    },
    {
      "name": "apiKey",
      "required": false,
      "type": "TEXT",
      "regexValidation": "^[a-zA-Z-]+$",
      "labelTranslations": [
        {
          "lang": "en",
          "value": "API Key"
        },
        {
          "lang": "de",
          "value": "API-Schl체ssel"
        }
      ],
      "placeholderTranslations": [
        {
          "lang": "en",
          "value": "An API key received in the confirmation mail"
        },
        {
          "lang": "de",
          "value": "API-Schl체ssel, den Sie in der Best채tigungsmail erhalten haben"
        }
      ]
    },
    {
      "name": "networkName",
      "required": true,
      "type": "TEXT",
      "regexValidation": "^[a-zA-Z-]+$",
      "labelTranslations": [
        {
          "lang": "en",
          "value": "Net name"
        },
        {
          "lang": "de",
          "value": "Der Netzwerkname"
        }
      ],
      "placeholderTranslations": [
        {
          "lang": "en",
          "value": "The network name"
        },
        {
          "lang": "de",
          "value": "Der Netzwerkname, den Sie in der Best채tigungsmail erhalten haben"
        }
      ]
    },
    {
      "name": "showPictures",
      "required": false,
      "type": "BOOLEAN",
      "labelTranslations": [
        {
          "lang": "en",
          "value": "Show pictures"
        }
      ]
    },
    {
      "name": "enableLogging",
      "required": false,
      "type": "BOOLEAN",
      "labelTranslations": [
        {
          "lang": "en",
          "value": "Enable logging"
        }
      ]
    },
    {
      "name": "myFavouriteNumber",
      "required": true,
      "type": "TEXT",
      "regexValidation": "^[a-zA-Z-]+$",
      "labelTranslations": [
        {
          "lang": "en",
          "value": "Favourite number"
        }
      ],
      "placeholderTranslations": [
        {
          "lang": "en",
          "value": "My favourite number"
        }
      ]
    }],
    "shellNavigation": {
        "icon": "activities",
        "sideNavigation": [
          {
            "pathSegment": "home",
            "labelTranslations": [
              {
                "lang": "en",
                "value": "Home"
              }
            ],
            "viewUrl": "/#/",
            "icon": "home",
            "isDefault": true,
            "children": []
          }
       ]
    }
}
